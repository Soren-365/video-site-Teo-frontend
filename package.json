{
  "name": "with-apollo-and-redux",
  "version": "2.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node hostserver/server.js",
    "export": "next export",
    "postexport": "node scripts/postExport.js",
    "css-build": "node-sass --omit-source-map-url sass/mainstyles.scss css/mainstyles.css",
    "css-watch": "npm run css-build -- --watch",
    "dev-css": "npm run css-build && npm run dev -- -p 3000",
    "type-check": "tsc",
    "deploy": "npm run build && npm run export",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.5.1",
    "apollo-client-preset": "^1.0.4",
    "apollo-link-context": "^1.0.19",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "es6-promise": "^4.2.8",
    "graphql": "^14.1.1",
    "graphql-anywhere": "^4.0.2",
    "graphql-tag": "^2.5.0",
    "https-proxy-agent": "^2.2.2",
    "isomorphic-unfetch": "^3.0.0",
    "morgan": "^1.9.1",
    "next": "latest",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^2.1.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.6.0",
    "react": "^16.7.0",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^1.1.1",
    "redux-thunk": "^2.2.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "faker": "^4.1.0",
    "webpack-bundle-analyzer": "^3.5.1"
  }
}
